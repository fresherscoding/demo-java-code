pipeline {
    agent any

    stages {
         stage('Build') {
            steps {
                dir("demo-java-code/simple-code"){
			sh 'mvn clean install'
		}
            }
        }

        stage('Test') {
            steps {
                dir("demo-java-code/simple-code"){
			sh 'mvn test'
		}
            }
        }
		
	stage('Complete'){
		steps{
			echo "Project setup is done."
		}
	}
    }
}
